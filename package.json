{
  "name": "vue-select-image",
  "description": "Vue 2 Component for selecting image from list",
  "version": "1.7.0",
  "license": "MIT",
  "author": "Irfan Maulana (https://github.com/mazipan/)",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --open",
    "build-demo": "cross-env NODE_ENV=production webpack --mode production --progress --hide-modules",
    "build-lib": "cross-env NODE_ENV=production webpack --mode production --progress --hide-modules --config webpack.config.dist.js",
    "publish-demo": "node ./publish-demo.js",
    "test": "./node_modules/.bin/jest --coverage",
    "dist": "rm -rf dist && rm -rf dist-demo && npm run build-demo && npm run build-lib && npm run publish-demo"
  },
  "main": "dist/vue-select-image.js",
  "files": [
    "dist/vue-select-image.js",
    "dist/vue-select-image.js.gz",
    "dist/vue-select-image.css",
    "dist/vue-select-image.css.gz"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mazipan/vue-select-image.git"
  },
  "bugs": {
    "url": "https://github.com/mazipan/vue-select-image/issues"
  },
  "keywords": [
    "vue select image",
    "vue image select",
    "vue image picker",
    "vue2 image picker",
    "vue2 image select"
  ],
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "avoriaz": "^6.3.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "uglifyjs-webpack-plugin": "1.3.0",
    "optimize-css-assets-webpack-plugin": "4.0.2",
    "mini-css-extract-plugin": "0.4.2",
    "file-loader": "^2.0.0",
    "jest": "^23.6.0",
    "gh-pages": "2.1.1",
    "vue-jest": "^2.6.0",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "vue-ionicons": "^1.1.4",
    "vue-loader": "^15.4.2",
    "vue-highlightjs": "1.3.3",
    "vue-google-adsense": "^1.2.0",
    "vue-script2": "^2.0.3",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-serve": "^2.0.2"
  }
